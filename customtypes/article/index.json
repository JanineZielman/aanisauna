        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": "URL slug for the article"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "SubTitle",
          "placeholder": ""
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "time": {
        "type": "Text",
        "config": {
          "label": "Time",
          "placeholder": ""
        }
      },
      "location": {
        "type": "StructuredText",
        "config": {
          "label": "Location",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink"
        }
      },
      "color": {
        "type": "Color",
        "config": {
          "label": "Color",
          "placeholder": ""
